#
# data for html4.01 h4 tag
#

@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.

@prefix h: <../html.n3#>.
@prefix t: <../tags.n3#>.
@prefix a: <../attributes.n3#>.
@prefix p: <../types.n3#>.
@prefix s: <spec.n3#>.

@prefix : <#>.

:h4        a            h:Definition;
           h:definedIn  s:spec;
           h:for        t:h4.

:lang      a            h:Containment;
           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:lang;
           h:type       t:LanguageCode;
           h:mandatory  h:IMPLIED;
           rdfs:comment "the base language of an element's attribute values and text content".

:dir       a            h:Containment;
           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:dir;
           h:type       t:Direction;
           h:mandatory  h:IMPLIED;
           rdfs:comment "the base direction of directionally neutral text".

:id        a            h:Containment;
           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:id;
           h:type       t:ID;
           h:mandatory  h:IMPLIED;
           rdfs:comment "document-wide unique id".

:class     a            h:Containment;
           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:class;
           h:type       t:CDATA;
           h:mandatory  h:IMPLIED;

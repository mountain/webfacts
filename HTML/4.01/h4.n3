           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:onkeydown;
           h:type       t:Script;
           h:mandatory  h:IMPLIED;
           rdfs:comment "a key was pressed down".

:onkeyup   a            h:Containment;
           h:definedIn  s:spec;
           h:container  t:h4;
           h:containee  a:onkeyup;
           h:type       t:Script;
           h:mandatory  h:IMPLIED;
           rdfs:comment "a key was released".
